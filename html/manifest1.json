{
    "name": "Jit AppSec Agent",
    "url": "https://jit.io",
    "hook_attributes": {
        "url": "https://pr_agent_base_url/webhook/pr-agent/vendor/github/app/app_id"
    },
    "redirect_url": "__redirectUrl__",
    "callback_urls": [
        "https://api.jit.io/oauth/callback/vendor/github/app/<app_id>"
    ],
    "public": false,
    "default_permissions": {
        "actions": "write",
        "administration": "read",
        "organization_administration": "read",
        "checks": "write",
        "contents": "write",
        "deployments": "read",
        "issues": "write",
        "metadata": "read",
        "pull_requests": "write",
        "repository_hooks": "write",
        "workflows": "write",
        "members": "read",
        "organization_hooks": "write",
        "emails": "read"
    },
    "default_events": [
        "repository"
    ]
}